/*
给定 nums = [2, 7, 11, 15], target = 9
因为 nums[0] + nums[1] = 2 + 7 = 9
所以返回 [0, 1]
*/

class Solution {
    public int[] twoSum(int[] nums, int target) {
        for(int i = 0; i<num.length; i++){
          for(int j = i+1; j<num.length-1; j++){
            if(num[i]+num[j]==target){
              return [i,j];
            }
          }
        }
        return null;
    }
}
